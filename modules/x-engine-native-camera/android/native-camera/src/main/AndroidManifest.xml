<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nativ.camera">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <application>

        <meta-data
            android:name="com.zkty.native.camera"
            android:value="com.zkty.nativ.camera.NativeCamera" />
        <activity
            android:name="com.zkty.nativ.camera.cameraImpl.activity.ImagePickerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.zkty.nativ.camera.cameraImpl.activity.ImagePreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>
</manifest>
